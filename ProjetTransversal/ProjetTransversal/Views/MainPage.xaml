<Page x:Class="ProjetTransversal.Views.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:Core="using:Microsoft.Xaml.Interactions.Core"
      xmlns:Interactivity="using:Microsoft.Xaml.Interactivity"
      xmlns:controls="using:Template10.Controls"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="using:ProjetTransversal.Views"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:vm="using:ProjetTransversal.ViewModels"
      DataContext="{Binding Source={StaticResource Locator}, Path=MainPage}"
      mc:Ignorable="d">

    <StackPanel Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" Orientation="Vertical">

        <TextBlock x:Name="username" HorizontalAlignment="Left" Height="20" Text="{Binding username}" VerticalAlignment="Top" Width="61.6666679382324"/>
        <TextBlock x:Name="password" Text="{Binding password}"/>
    </StackPanel>

</Page>

